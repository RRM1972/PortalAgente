                                                }
                                                <tr class="text-white text-center" style="background-color:#841a66;">
                                                    <th colspan="7">Trimestral</th>
                                                </tr>
                                                @foreach (var oPrimas in Model.ListaPrimasTrimestralLegacy)
                                                {
                                                    if (oPrimas.Cobertura != "2")
                                                    {
                                                        <tr>
                                                            @{
                                                                if (oPrimas.Cobertura == "1")
                                                                {
                                                                    if (oPrimas.TipoPersona == "01")
                                                                    {
                                                                        <th scope="row">Titular</th>
                                                                    }
                                                                    else
                                                                    {
                                                                        <th scope="row">Conyuge</th>
                                                                    }

                                                                }

                                                                if (oPrimas.Cobertura == "3")
                                                                {
                                                                    <th scope="row">Dependientes</th>
                                                                }

                                                                if (oPrimas.Cobertura == "4")
                                                                {
                                                                    <th scope="row">Complicaciones Maternidad</th>
                                                                }
                                                                if (oPrimas.Cobertura == "5")
                                                                {
                                                                    <th scope="row">Trasplantes Organos</th>
                                                                }
                                                                if (oPrimas.Cobertura == "CAD")
                                                                {
                                                                    <th scope="row">Costo Administrativo</th>
                                                                }
                                                                if (oPrimas.Cobertura == "TOT")
                                                                {
                                                                    <th scope="row" class="bg-secondary bg-soft"><h6>Total 1er Pago</h6></th>
                                                                }
                                                                if (oPrimas.Cobertura == "STO")
                                                                {
                                                                    <th scope="row" class="bg-secondary bg-soft"><h6>Total 2do, 3er, 4to Pago</h6></th>
                                                                }

                                                            }
                                                            @if (oPrimas.Cobertura == "TOT" || oPrimas.Cobertura == "STO")
                                                            {
                                                                <td class="text-center bg-secondary bg-soft"><h6>@oPrimas.Opcion1.ToString()</h6></td>
                                                                <td class="text-center bg-secondary bg-soft"><h6>@oPrimas.Opcion2.ToString()</h6></td>
                                                                <td class="text-center bg-secondary bg-soft"><h6>@oPrimas.Opcion3.ToString()</h6></td>
                                                                <td class="text-center bg-secondary bg-soft"><h6>@oPrimas.Opcion4.ToString()</h6></td>
                                                                <td class="text-center bg-secondary bg-soft"><h6>@oPrimas.Opcion5.ToString()</h6></td>
                                                                <td class="text-center bg-secondary bg-soft"><h6>@oPrimas.Opcion6.ToString()</h6></td>
                                                            }
                                                            else
                                                            {
                                                                <td class="text-center">@oPrimas.Opcion1.ToString()</td>
                                                                <td class="text-center">@oPrimas.Opcion2.ToString()</td>
                                                                <td class="text-center">@oPrimas.Opcion3.ToString()</td>
                                                                <td class="text-center">@oPrimas.Opcion4.ToString()</td>
                                                                <td class="text-center">@oPrimas.Opcion5.ToString()</td>
                                                                <td class="text-center">@oPrimas.Opcion6.ToString()</td>
                                                            }

                                                        </tr>
                                                    }
                                                }
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Envio de Correo-->

    <div class="offcanvas offcanvas-start" tabindex="-1" id="offcanvasCorreo" aria-labelledby="offcanvasCorreo">
        <div class="offcanvas-header">
            <h5 class="offcanvas-title" id="offcanvasExampleLabel">Enviar Cotizacion</h5>
            <button type="button" class="btn-close text-reset" data-bs-dismiss="offcanvas" aria-label="Close"></button>
        </div>
        <div class="offcanvas-body">
            <div class="card">
                <div class="card-body">
                    <div class="col-md-12 mb-3">
                        <label class="form-label">Selecciona Producto</label>

                        <select class="form-select" name="CodigoProducto" id="CodigoProducto">
                            <option value=1>Beyond</option>
                            <option value=2>Privilege</option>
                            <option value=3>Liberty</option>
                            <option value=4>Legacy</option>
                        </select>
                    </div>
                    <div class="col-md-12 mb-3">
                        <label class="form-label">Selecciona Correo</label>

                        <select class="form-select" name="CodigoProducto" id="CodigoProducto">
                            <option value=1 selected>Correo1@gmail.com</option>
                            <option value=2>Correo2@gmail.com</option>
                            <option value=3>Correo3@gmail.com</option>
                            <option value=1>Otro Correo</option>
                        </select>
                    </div>
                    <div class="col-md-12 mb-3">
                        <label for="NombreSolicitante" class="form-label">Otro Correo</label>
                        <input type="text" class="form-control" placeholder="Otro Correo..." name="OtroCorreo" id="OtroCorreo" maxlength="100" value="" required>
                    </div>
                    <div class="col-md-12 mb-3">
                        <button type="button" class="btn btn-primary waves-effect waves-light">Enviar Correo</button>
                    </div>

                </div>
            </div>
        </div>
    </div>

    <div class="offcanvas offcanvas-start" tabindex="-1" id="offcanvasAprobar" aria-labelledby="offcanvasAprobar">
        <div class="offcanvas-header">
            <h5 class="offcanvas-title" id="offcanvasExampleLabel">Aprobar Cotizacion</h5>
            <button type="button" class="btn-close text-reset" data-bs-dismiss="offcanvas" aria-label="Close"></button>
        </div>
        <div class="offcanvas-body">
            <div class="card">
                <div class="card-body">
                    <div class="col-md-12 mb-3">
                        <label class="form-label">Selecciona Producto</label>

                        <select class="form-select" name="CodigoProducto" id="CodigoProducto">
                            <option value=1>Beyond</option>
                            <option value=2>Privilege</option>
                            <option value=3>Liberty</option>
                            <option value=4>Legacy</option>
                        </select>
                    </div>
                    <div class="col-md-12 mb-3">
                        <label class="form-label">Selecciona Producto</label>

                        <select class="form-select" name="CodigoProducto" id="CodigoProducto">
                            <option value=1>Opción I $500 - $1,000</option>
                            <option value=1>Opción II $1,000 - $2,000</option>
                            <option value=1>Opción III $2,000 - $3,000</option>
                            <option value=1>Opción IV $5,000</option>
                            <option value=1>Opción V $10,000</option>
                            <option value=1>Opción V $20,000</option>
                        </select>
                    </div>
                    <div class="col-md-12 mb-3">
                        <label class="form-label">Selecciona Forma de Pago</label>

                        <select class="form-select" name="CodigoProducto" id="CodigoProducto">
                            <option value=1 selected>Anual</option>
                            <option value=2>Semestral</option>
                            <option value=3>Trimestral</option>
                        </select>
                    </div>
                    <div class="col-md-12 mb-3">
                        <button type="button" class="btn btn-primary waves-effect waves-light" onclick="location.href='@Url.Action("Consulta", "Certificados",new { nCodigoCertificado = 229 })'">Aprobar Cotización</button>
                    </div>

                </div>
            </div>
        </div>
    </div>

</body>
</html>

