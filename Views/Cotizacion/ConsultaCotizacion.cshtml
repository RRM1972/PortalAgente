@model LoyalIGWEB.Models.ConsultaCotizacion
@{
    ViewBag.Title = "Consulta Cotización";
    ViewBag.pTitle = "Consulta Cotización";
    ViewBag.pageTitle = "Consulta Cotización";
    Layout = "~/Views/Shared/Layouts/_layout_horizontal.cshtml";
}

@section styles{
    <link href="~/assets/libs/select2/css/select2.min.css" rel="stylesheet" type="text/css" />
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Consulta Cotización</title>
</head>

