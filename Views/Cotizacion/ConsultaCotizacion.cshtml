        <div class="col-lg-4">
            <div class="card">
                <div class="card-body">
                    <div class="text-center card-title bg-primary text-white rounded">
                        <h4 class="text-white">Cotizacion Nro. @Model.CodigoCotizacion</h4>
                    </div>
                    <div class="card-title rounded">
                        Datos Solicitante
                    </div>
                    <div class="dropdown-divider"></div>
                    <div class="row">
                        <div><h5>@Model.NombreSolicitante</h5></div>
                    </div>
                    <div class="row">
                        @if (Model.SexoSolicitante == "M")
                        {<div><h5>Masculino</h5></div>}
                        else
                        { <div><h5>Femenino</h5></div>}

                    </div>
                    <div class="row">
                        <div><h5>@Model.FechaNacimnientoSolicitante.ToString()</h5></div>
                    </div>
                    <div class="row">
                        <div><h5>@Model.EdadSolicitante.ToString() Años</h5></div>
                    </div>
                    <div class="row">
                        <div><h5>@Model.Correo.ToString()</h5></div>
                    </div>
                </div>

            </div>
            @if (Model.FechaNacimnientoConyuge != "")
            {
                <div class="card">
                    <div class="card-body">
                        <div class="card-title rounded">
                            Datos Conyuge
                        </div>
                        <div class="dropdown-divider"></div>
                        <div class="row">
                            @if (Model.SexoConyuge == "M")
                            {<div><h5>Masculino</h5></div>}
                            else
                            { <div><h5>Femenino</h5></div>}

                        </div>
                        <div class="row">
                            <div><h5>@Model.FechaNacimnientoConyuge.ToString()</h5></div>
                        </div>
                        <div class="row">
                            <div><h5>@Model.EdadConyuge.ToString() Años</h5></div>
                        </div>
                    </div>

                </div>}

